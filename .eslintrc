{
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true
  },
  "rules": {
    "quotes": [1, "single"],
    "no-spaced-func": 0,
    "no-undef": 0,
    "no-console": 0,
    "new-cap": 0,
    "no-wrap-func": 0,
    "no-unused-vars": 0
  }
}
