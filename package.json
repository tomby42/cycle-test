{
    "name": "balfour-pages",
    "version": "0.1.0",
    "description": "Balfour pages",
    "engines": {
        "iojs": ">=1.0",
        "node": ">=0.10"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/salsita/balfour-pages.git"
    },
    "author": "Salsita Software",
    "license": "",
    "scripts": {
        "build": "npm run build:clean && npm run build:app",
        "build:clean": "rm -rf dist/ build/",
        "build:app": "webpack",
        "start": "NODE_ENV=development webpack-dev-server",
        "lint": "eslint .",
        "karma": "karma start ./karma.conf.js",
        "test": "npm run karma --  --single-run --reporters nyan",
        "test:dev": "npm run karma -- --no-single-run --auto-watch --reporters nyan",
        "test:cov": "COVERAGE=1 npm run karma -- --single-run --reporters coverage",
        "check": "npm run lint && npm run test && npm run build"
    },
    "dependencies": {
        "babel-core": "^5.8.3",
        "babel-eslint": "^3.1.26",
        "babel-loader": "^5.3.2",
        "css-loader": "^0.15.5",
        "less-loader": "^2.0.0",
        "eslint": "^0.24.1",
        "eslint-loader": "^0.14.2",
        "eslint-plugin-react": "^3.0.0",
        "file-loader": "^0.8.4",
        "html-webpack-plugin": "^1.6.0",
        "inject-loader": "^2.0.0",
        "isparta-loader": "^0.2.0",
        "jasmine-core": "^2.3.4",
        "karma": "^0.13.2",
        "karma-coverage": "^0.4.2",
        "karma-jasmine": "^0.3.6",
        "karma-junit-reporter": "^0.3.2",
        "karma-nyan-reporter": "0.2.1",
        "karma-phantomjs-launcher": "^0.2.0",
        "karma-webpack": "^1.6.0",
        "lodash": "^3.10.0",
        "node-libs-browser": "^0.5.2",
        "null-loader": "^0.1.1",
        "phantomjs": "^1.9.17",
        "react": "^0.13.3",
        "react-hot-loader": "^1.2.8",
        "react-jasmine-matchers": "^2.0.0",
        "sorted-object": "^1.0.0",
        "style-loader": "^0.12.3",
        "url-loader": "^0.5.6",
        "webpack": "^1.10.1",
        "webpack-dev-server": "^1.10.1",
        "cycle-react": "^2.0.0",
        "immutable": "3.7.4",
        "js-atom": "0.4.0",
        "transducers-js": "0.4.174",
        "bootstrap": "^3.3.5",
        "normalize.css": "~3.0.3"
    }
}
